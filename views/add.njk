{% extends "layout.njk" %}

{% block pagina %}

<h1>Cadastro</h1>
<div class="col-md-6">
  <form action="/" method="post">
    <div class="form-group">
      <label for="time">Ddata e Hora</label>
      <input type="datetime-local" class="form-control" name="time" id="time" placeholder="Time">
    </div>
    <div class="form-group">
      <label for="lat">Latitude</label>
      <input type="number" step="0.01" class="form-control" name="lat" id="lat" placeholder="Latitude">
    </div>
    <div class="form-group">
      <label for="lng">Longitude</label>
      <input type="number" step="0.01" class="form-control" name="lng" id="lng" placeholder="Longitude">
    </div>
    <div class="form-group">
      <label for="so">Sistema Operacional</label>
      <input type="text" class="form-control" name="so" id="so" placeholder="Sistema Operacional">
    </div>
    <div class="form-group">
      <label for="version">Vers찾o</label>
      <input type="text" class="form-control" name="version" id="version" placeholder="Vers찾o">
    </div>
    <div class="form-group">
      <label for="model">Modelo</label>
      <input type="text" class="form-control" name="model" id="model" placeholder="Modelo">
    </div>
    <div class="form-group">
      <label for="macaddress">MAC Address</label>
      <input type="text" class="form-control" name="macaddress" id="macaddress" placeholder="MAC Address">
    </div>
    <div class="form-group">
      <label for="connections">Conex천es Ativas</label>
      <input type="text" class="form-control" name="connections" id="connections" placeholder="Conex천es Ativas">
    </div>

    <button type="submit" class="btn btn-primary">Cadastrar</button>
    <a href="#" class="btn btn-default">Voltar</a>
  </form>
</div>



{% endblock %}
